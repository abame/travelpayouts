{
    "name": "abame/travel-payouts",
    "description": "PHP SDK for travelpayouts.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Albion Bame",
            "email": "albionbame@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle":"7.4.*",
        "it-for-free/array_column": "v1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "phpunit/php-invoker" : "3.1.*",
        "squizlabs/php_codesniffer": "3.6.*",
        "phpstan/phpstan": "1.2.*",
        "phpmd/phpmd": "2.10.*",
        "friendsofphp/php-cs-fixer": "v3.3.*",
        "slevomat/coding-standard": "7.0.*",
        "vimeo/psalm": "^4.14",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "TravelPayouts\\": "src/travelPayouts"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\TravelPayouts\\" : "tests/travelPayouts"
        }
    }
}
